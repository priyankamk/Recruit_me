<div class="jumbotron">
  <table class="table table-striped">
    <thead class="thead-dark">
      <tr>
        <th>Company Name</th>
        <th>Job Title</th>
        <th>Location</th>
      </tr>
    </thead>
    <tbody>
      <% @jobs.each do |job| %>
        <tr>
          <td>
            <a href="/jobs/<%= job.id %>"><%= job.company_name %> </a>
          </td>
          <td> <%= job.job_title %> </td>
          <td> <%= job.location %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
